body{font:16px sans-serif;line-height:1.5rem;margin:auto;max-width:50rem;padding:0 5%}blockquote{border-left:6px solid #bbb;margin-left:0;padding-left:1.5rem}h1,h2,h3{line-height:1.75rem}nav ul{font-size:1.25rem}nav li{padding-right:.5rem}.unstyled-list{padding:0}.unstyled-list li{list-style-type:none;margin-bottom:1rem}.home{font-weight:700}@media(min-width:32rem){nav li{display:inline}}img:not(.u-photo){display:block;height:auto;margin:auto;max-width:100%}.u-photo{display:inline-block;vertical-align:-.1em}pre,code{border:1px solid #bbb;padding:0 .25rem}pre code{border:0;padding:0}pre{overflow:auto visible;padding:.5rem}